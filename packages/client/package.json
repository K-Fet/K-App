{
  "name": "k-app-client",
  "version": "2.3.6",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --disable-host-check",
    "build": "ng build --aot",
    "build:prod": "ng build --prod",
    "prestart": "yarn run gitversion",
    "prebuild": "yarn run gitversion",
    "prebuild:prod": "yarn run gitversion && yarn run injectenv",
    "postinstall": "ngcc --source ../../node_modules --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "gitversion": "node scripts/git.version.js",
    "injectenv": "node scripts/inject.env.js"
  },
  "dependencies": {
    "@angular/animations": "^9.1.1",
    "@angular/cdk": "^9.2.0",
    "@angular/common": "^9.1.1",
    "@angular/compiler": "^9.1.1",
    "@angular/core": "^9.1.1",
    "@angular/flex-layout": "^9.0.0-beta.29",
    "@angular/forms": "^9.1.1",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "^9.1.1",
    "@angular/platform-browser-dynamic": "^9.1.1",
    "@angular/router": "^9.1.1",
    "@bugsnag/js": "^6.5.2",
    "@bugsnag/plugin-angular": "^6.5.2",
    "@danielmoncada/angular-datetime-picker": "^9.3.0",
    "@k-fet/ng-dynamic-forms-core": "^10.1.0",
    "@k-fet/ng-dynamic-forms-ui-material": "^10.1.0",
    "angular-imask": "^6.0.4",
    "angular2-recaptcha": "^1.1.0",
    "core-js": "^3.6.5",
    "date-fns": "^2.12.0",
    "jwt-decode": "^2.2.0",
    "ngx-date-fns": "^6.2.1",
    "ngx-permissions": "^7.0.3",
    "rxjs": "^6.5.5",
    "tslib": "^1.11.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.0",
    "@angular/cli": "^9.1.1",
    "@angular/compiler-cli": "^9.1.1",
    "@angular/language-service": "^9.1.1",
    "@types/hammerjs": "^2.0.36",
    "@types/node": "^12.12.35",
    "dotenv": "^8.2.0",
    "ts-node": "~8.8.2",
    "typescript": "~3.8.3"
  }
}
