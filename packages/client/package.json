{
  "name": "k-app-front",
  "version": "2.3.7",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config proxy.conf.json --host 0.0.0.0 --disable-host-check",
    "build": "ng build --aot",
    "build:prod": "ng build --prod",
    "prestart": "yarn run gitversion",
    "prebuild": "yarn run gitversion",
    "prebuild:prod": "yarn run gitversion && yarn run injectenv",
    "gitversion": "node scripts/git.version.js",
    "injectenv": "node scripts/inject.env.js",
    "lint:ts": "ng lint",
    "lint:css": "stylelint \"src/**/*.scss\"",
    "lint": "yarn run lint:ts && yarn run lint:css"
  },
  "dependencies": {
    "@angular/animations": "^8.2.10",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^8.2.10",
    "@angular/compiler": "^8.2.10",
    "@angular/core": "^8.2.10",
    "@angular/flex-layout": "^8.0.0-beta.27",
    "@angular/forms": "^8.2.10",
    "@angular/material": "^8.2.3",
    "@angular/platform-browser": "^8.2.10",
    "@angular/platform-browser-dynamic": "^8.2.10",
    "@angular/router": "^8.2.10",
    "@bugsnag/js": "^6.4.2",
    "@bugsnag/plugin-angular": "^6.4.2",
    "@ng-dynamic-forms/core": "^8.1.0",
    "@ng-dynamic-forms/ui-material": "^8.1.0",
    "angular2-recaptcha": "^1.1.0",
    "angular2-text-mask": "^9.0.0",
    "core-js": "^3.2.1",
    "date-fns": "^2.4.1",
    "hammerjs": "^2.0.8",
    "jwt-decode": "^2.2.0",
    "ng-pick-datetime": "^7.0.0",
    "ngx-date-fns": "^5.0.0",
    "ngx-permissions": "^7.0.3",
    "rxjs": "^6.5.2",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.8",
    "@angular/cli": "^8.1.2",
    "@angular/compiler-cli": "^8.1.2",
    "@angular/language-service": "^8.1.2",
    "@types/hammerjs": "^2.0.36",
    "@types/node": "~12.7.12",
    "codelyzer": "^5.1.0",
    "dotenv": "^8.1.0",
    "stylelint": "^11.1.1",
    "stylelint-config-airbnb": "^0.0.0",
    "stylelint-order": "^3.0.1",
    "stylelint-scss": "^3.9.2",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.0",
    "tslint-config-airbnb": "^5.11.1",
    "typescript": "~3.4.5"
  }
