{
  "name": "k-app-server",
  "version": "2.3.5",
  "private": true,
  "scripts": {
    "build": "yarn --cwd client/ build",
    "build:prod": "yarn --cwd client/ build:prod",
    "cli": "node tools/cli/index.js",
    "dev": "concurrently --kill-others \"yarn:dev:*\"",
    "dev:back": "nodemon --watch server --inspect",
    "dev:front": "yarn --cwd client/ start",
    "eslint": "eslint server/",
    "test": "concurrently \"yarn:test:*\"",
    "test:lint": "eslint server/ && yarn --cwd client/ lint",
    "test:back:it": "jest int",
    "test:back:unit": "jest unit --coverage",
    "test:front": "yarn --cwd client/ build",
    "preversion": "yarn test",
    "postversion": "git push --follow-tags && opn https://github.com/K-Fet/K-App/releases/new"
  },
  "dependencies": {
    "argon2": "0.23.0",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "date-fns": "^2.0.0-alpha.27",
    "dotenv": "^8.0.0",
    "express": "^4.16.4",
    "express-joi-validation": "^0.3.0",
    "express-jwt": "^5.3.1",
    "express-jwt-permissions": "^1.2.3",
    "express-rate-limit": "^2.14.2",
    "express-x-hub": "^1.0.4",
    "inquirer": "^6.2.2",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.4.0",
    "moleculer": "^0.13.6",
    "moleculer-db": "0.7.9",
    "moleculer-db-adapter-mongoose": "^0.7.2",
    "moleculer-middleware-permissions": "^1.3.0",
    "moleculer-web": "moleculerjs/moleculer-web",
    "mongoose": "^5.4.14",
    "morgan": "^1.9.1",
    "mysql2": "^1.6.5",
    "nconf": "^0.10.0",
    "nodemailer": "^6.2.1",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "sequelize": "^4.42.0",
    "umzug": "^2.2.0",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "codecov": "^3.0.4",
    "concurrently": "^4.1.0",
    "eslint": "^5.14.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "jest": "^24.1.0",
    "jest-environment-node": "^24.0.0",
    "jest-extended": "^0.11.1",
    "ngrok": "^3.1.1",
    "nodemon": "^1.18.10",
    "opn-cli": "^4.0.0",
    "sqlite3": "^4.0.6",
    "supertest": "^4.0.2"
  },
  "main": "server/index.js"
}
