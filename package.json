{
  "name": "k-app",
  "version": "2.1.1",
  "description": "K-App provide a large panel of useful tools for associations",
  "contributors": [
    "Corentin Giraud <corentin.giraud@insa-lyon.fr>",
    "Marc-Antoine Fernandes <marcantoinefernandes@gmail.com>",
    "Baptiste Viry",
    "ClÃ©ment Guittat",
    "Mickael Gadou"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/K-Fet/K-App"
  },
  "bugs": {
    "url": "https://github.com/K-Fet/K-App/issues"
  },
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "build": "cd client/ && yarn run build:prod",
    "build:dev": "cd client/ && yarn run build",
    "cli": "node tools/cli/index.js",
    "dev": "concurrently --kill-others \"npm:dev:*\"",
    "dev:back": "nodemon --watch server --watch server/index.js --inspect",
    "dev:front": "cd client/ && yarn start",
    "eslint": "eslint server/",
    "eslint:watch": "esw --watch server/",
    "lint": "yarn run eslint && cd client/ && yarn run lint",
    "install:prod": "yarn install --production --loglevel warn",
    "postinstall": "cd client/ && yarn install --production=false",
    "test": "concurrently \"npm:test:*\"",
    "test:back:it": "jest int",
    "test:back:unit": "jest unit --coverage",
    "test:front": "cd client/ && yarn build",
    "preversion": "yarn test && yarn run build"
  },
  "dependencies": {
    "argon2": "0.19.3",
    "body-parser": "^1.18.3",
    "compression": "^1.7.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-joi-validation": "^0.2.1",
    "express-jwt": "^5.3.1",
    "express-jwt-permissions": "^1.2.1",
    "express-rate-limit": "^2.12.2",
    "express-x-hub": "^1.0.4",
    "inquirer": "^6.2.0",
    "joi": "^13.6.0",
    "jsonwebtoken": "^8.3.0",
    "morgan": "^1.9.0",
    "mysql2": "^1.6.1",
    "nconf": "^0.10.0",
    "nodemailer": "^4.6.8",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "sequelize": "^4.38.0",
    "umzug": "^2.1.0",
    "uuid": "^3.3.2",
    "winston": "^3.1.0"
  },
  "devDependencies": {
    "codecov": "^3.0.4",
    "concurrently": "^4.0.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.21.0",
    "jest": "^23.5.0",
    "jest-environment-node": "^23.4.0",
    "jest-extended": "^0.8.1",
    "ngrok": "^3.0.1",
    "nodemon": "^1.18.3",
    "opn": "^5.3.0",
    "sqlite3": "^4.0.2",
    "supertest": "^3.1.0"
  },
  "main": "server/index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "client/",
      "tools/"
    ],
    "delay": "2500"
  },
  "jest": {
    "collectCoverageFrom": [
      "<rootDir>/server/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "app/models/"
    ],
    "setupTestFrameworkScriptFile": "jest-extended",
    "testEnvironment": "node"
  }
}
